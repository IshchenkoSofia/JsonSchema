{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "triggerId",
    "isActive",
    "hasExceededAttempts",
    "isPossibleToWin"
  ],
  "properties": {
    "triggerId": {
      "$id": "#/properties/triggerId",
      "type": "integer",
      "title": "The Triggerid Schema",
      "default": 0,
      "examples": [
        111
      ]
    },
    "isActive": {
      "$id": "#/properties/isActive",
      "type": "boolean",
      "title": "The Isactive Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "hasExceededAttempts": {
      "$id": "#/properties/hasExceededAttempts",
      "type": "boolean",
      "title": "The Hasexceededattempts Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "isPossibleToWin": {
      "$id": "#/properties/isPossibleToWin",
      "type": "boolean",
      "title": "The Ispossibletowin Schema",
      "default": false,
      "examples": [
        true
      ]
    }
  }
}